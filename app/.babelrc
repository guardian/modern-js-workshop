{
    "presets": [
        [
            "@babel/env",
            {
                "debug": true,
                "targets": {
                    "browsers": ["> 1%", "last 2 versions", "IE >= 9"]
                },
                "modules": false,
                "useBuiltIns": "usage"
            }
        ]
    ],
    "plugins": ["@babel/plugin-proposal-object-rest-spread"],
    "env": {
        "test": {
            "presets": [
                [
                    "@babel/env",
                    {
                        "useBuiltIns": "usage"
                    }
                ]
            ]
        }
    }
}
